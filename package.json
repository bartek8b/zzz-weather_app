{
  "name": "webpack-template",
  "version": "1.0.0",
  "description": "Template repo for projects using webpack",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack serve",
    "set-branch": "git branch gh-pages",
    "change-branch": "git checkout gh-pages && git merge main --no-edit",
    "checkout": "git checkout main",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "lint-and-format": "npm run lint:fix && npm run format",
    "build": "webpack",
    "commit-dist": "git add dist -f && git commit -m \"Deployment commit\"",
    "push-gh-pages": "git subtree push --prefix dist origin gh-pages",
    "force-push-gh-pages": "git push origin `git subtree split --prefix dist main`:gh-pages --force,"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bartek8b/webpack-template.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bartek8b/webpack-template/issues"
  },
  "homepage": "https://github.com/bartek8b/webpack-template#readme",
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.3.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "prettier": "3.6.2",
    "style-loader": "^4.0.0",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
